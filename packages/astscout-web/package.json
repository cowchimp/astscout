  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "prettier": "^1.12.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
